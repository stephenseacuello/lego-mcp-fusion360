{
  "name": "LEGO Laser Engraving Presets",
  "version": "1.0.0",
  "description": "Laser power and speed settings for engraving LEGO bricks",

  "safety_notes": [
    "ABS plastic releases toxic fumes when laser engraved - use proper ventilation!",
    "Always use air assist to prevent flaming and improve cut quality",
    "Never leave laser unattended",
    "Wear appropriate laser safety glasses for your wavelength"
  ],

  "presets": {
    "abs_engrave_light": {
      "name": "Light Surface Mark",
      "description": "Shallow surface marking for subtle text/logos",
      "material": "abs",
      "power_percent": 15,
      "speed_mm_min": 1000,
      "passes": 1,
      "dpi": 254,
      "expected_depth_mm": 0.05,
      "air_assist": true,
      "focus_offset_mm": 0,
      "line_interval_mm": 0.1
    },
    "abs_engrave_medium": {
      "name": "Medium Engrave",
      "description": "Standard engraving depth for clear visibility",
      "material": "abs",
      "power_percent": 20,
      "speed_mm_min": 800,
      "passes": 1,
      "dpi": 254,
      "expected_depth_mm": 0.1,
      "air_assist": true,
      "focus_offset_mm": 0,
      "line_interval_mm": 0.1
    },
    "abs_engrave_deep": {
      "name": "Deep Engrave",
      "description": "Deeper engraving for tactile text/logos",
      "material": "abs",
      "power_percent": 25,
      "speed_mm_min": 500,
      "passes": 2,
      "dpi": 254,
      "expected_depth_mm": 0.2,
      "air_assist": true,
      "focus_offset_mm": 0,
      "line_interval_mm": 0.08,
      "pass_depth_offset_mm": 0.1
    },
    "abs_vector_light": {
      "name": "Vector Line - Light",
      "description": "Light vector line engraving",
      "material": "abs",
      "mode": "vector",
      "power_percent": 12,
      "speed_mm_min": 1500,
      "passes": 1,
      "air_assist": true
    },
    "abs_vector_medium": {
      "name": "Vector Line - Medium",
      "description": "Medium vector line engraving",
      "material": "abs",
      "mode": "vector",
      "power_percent": 18,
      "speed_mm_min": 1000,
      "passes": 1,
      "air_assist": true
    },
    "abs_cut_thin": {
      "name": "Cut Thin ABS (~1mm)",
      "description": "Cut through thin ABS sheet",
      "material": "abs",
      "mode": "cut",
      "power_percent": 80,
      "speed_mm_min": 200,
      "passes": 4,
      "air_assist": true,
      "focus_offset_mm": 0.5,
      "z_step_per_pass_mm": 0.25,
      "expected_cut_depth_mm": 1.0,
      "warning": "Multiple passes required. Generates significant fumes."
    },
    "pla_engrave_light": {
      "name": "PLA Light Engrave",
      "description": "Light engraving on PLA bricks",
      "material": "pla",
      "power_percent": 10,
      "speed_mm_min": 1200,
      "passes": 1,
      "air_assist": true,
      "notes": "PLA is easier to engrave than ABS but less heat resistant"
    },
    "pla_engrave_medium": {
      "name": "PLA Medium Engrave",
      "description": "Medium engraving on PLA bricks",
      "material": "pla",
      "power_percent": 15,
      "speed_mm_min": 800,
      "passes": 1,
      "air_assist": true
    }
  },

  "lego_applications": {
    "stud_text": {
      "description": "Engrave text on stud tops (like official LEGO)",
      "preset": "abs_engrave_light",
      "font_height_mm": 1.2,
      "font": "Arial Bold",
      "recommended_text": "LEGO",
      "notes": "Center on stud. Use vector mode for crisp text."
    },
    "serial_number": {
      "description": "Engrave serial/part numbers on underside",
      "preset": "abs_engrave_medium",
      "font_height_mm": 2.0,
      "location": "underside_cavity",
      "notes": "Useful for custom part identification"
    },
    "custom_logo": {
      "description": "Engrave custom logos on tile tops",
      "preset": "abs_engrave_medium",
      "max_size_mm": 12,
      "notes": "Best on tiles (no studs). Convert logo to SVG first."
    },
    "qr_code": {
      "description": "Engrave QR codes for part tracking",
      "preset": "abs_engrave_deep",
      "module_size_mm": 0.5,
      "min_size_mm": 10,
      "notes": "Requires high contrast. Test scan before production run."
    }
  },

  "material_compatibility": {
    "abs": {
      "laser_compatible": true,
      "engrave_quality": "good",
      "cut_quality": "fair",
      "fume_hazard": "high",
      "color_response": {
        "black": "excellent contrast",
        "white": "good contrast, may yellow",
        "red": "fair contrast",
        "yellow": "poor contrast, use higher power",
        "transparent": "poor, not recommended"
      }
    },
    "pla": {
      "laser_compatible": true,
      "engrave_quality": "excellent",
      "cut_quality": "good",
      "fume_hazard": "low",
      "notes": "Easier to engrave but prints may warp under laser heat"
    },
    "petg": {
      "laser_compatible": true,
      "engrave_quality": "fair",
      "cut_quality": "poor",
      "fume_hazard": "medium",
      "notes": "Tends to melt rather than ablate cleanly"
    },
    "delrin": {
      "laser_compatible": false,
      "notes": "Delrin releases formaldehyde when laser cut. DO NOT USE."
    }
  }
}
